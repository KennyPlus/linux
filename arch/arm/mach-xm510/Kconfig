menu "xm510 board feature"

config MACH_XM510
	bool "Support xm510 platform"
	select CPU_V7
	select PL330
	help
	  Includes support for the hisilion xm510 platform.

	  This includes specific configurations for the module and
	  its peripherals.

config DEFAULT_BUSCLK
	int "DEFAULT_BUSCLK"
	default "50000000"
	help
	  Default AHB clock rate

endmenu
